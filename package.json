{
  "name": "@life-palette/monorepo",
  "version": "0.0.3",
  "private": true,
  "description": "Life Palette Utils Monorepo",
  "repository": "https://github.com/IceyWu/life-palette-utils",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm -r --filter='./packages/*' run build",
    "dev": "pnpm --parallel -r run dev",
    "test": "pnpm -r run test",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint . --fix && prettier -w .",
    "play": "pnpm --filter playground dev",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "release": "node scripts/pre-release.js && pnpm --filter @life-palette/utils release && pnpm sync-changelog",
    "changelog": "changelogen --output CHANGELOG.md",
    "sync-changelog": "node scripts/sync-changelog.js"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "eslint-config-unjs": "^0.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.18.3",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
